{
    "version": 2,
    "functions": {
      "api/**/*.js": {
        "runtime": "vercel/node@20.x"
      }
    },
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1.js" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }